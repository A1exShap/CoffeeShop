@page "/test"

<PageTitle>Test</PageTitle>

<h1>Test</h1>

<p role="status">Current count: @currentString</p>

<button class="btn btn-primary" @onclick="IncrementCount">Click me</button>

@code {
    private string currentString = "";



    private void IncrementCount()
    {
        currentString += "!";
    }
}
